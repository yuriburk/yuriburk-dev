{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015", "es2017"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
    // "types": ["react"]
  },
  "include": ["src"],
  "exclude": [
    "./node_modules",
    "./public",
    "./.cache",
    "./gatsby-config.ts",
    "./gatsby-node.ts"
  ]
}
